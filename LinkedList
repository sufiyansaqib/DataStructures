#include <iostream>
using namespace std;

struct node{
  int data;
  node *next;
};

node *head = NULL;

void insertbeg(int x){
    node *temp = new node();
    temp->data = x;
    temp->next = head;
    head = temp;
}

void insertend(int x){
    node *temp = new node();
    temp->data = x;
    temp->next = NULL;
    node *temp1 = head;
    while(temp1->next != NULL){
        temp1 = temp1->next;
    }
    temp1->next = temp;
}

void display(){
    node *temp = head;
    while(temp != NULL){
        cout<<temp->data;
        temp = temp->next;
    }
}

int main()
{
    insertbeg(2);
    insertbeg(1);
    insertend(3);
    insertend(4);
    display();
    return 0;
}
